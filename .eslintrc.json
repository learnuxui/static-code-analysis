{
    "env": {
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            1,
            4
        ],
        "linebreak-style": [
            1,
            "windows"
        ],
        "quotes": [
          1,
            "single"
        ],
        "semi": [
          1,
            "always"
        ],
        "no-undef": [
            1
        ]
    }
}