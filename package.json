{
  "name": "static-code-check",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "chartist": "^0.10.1",
    "jslint": "^0.12.0",
    "jshint": "^2.9.5",
    "eslint-detailed-reporter": "^0.7.3"
  },
  "scripts": {
    "ES-LINT": "eslint src -f node_modules/eslint-detailed-reporter/lib/detailed.js -o ./report.html",
    "JS-LINT": "jslint --conf .jslint.conf *.js **/*.js",
    "JS-HINT": "jshint --config .jshintrc src || true"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-detailed-reporter": "^0.7.3",
    "jshint": "^2.9.5"
  }
}
